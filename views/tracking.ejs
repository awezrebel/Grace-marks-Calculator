<table id="table" >  
<thread>  
<tr>    
<form method="GET", action="http://localhost:8000/">     
</tr>  

 

  <h2>
Tracking of Paper Publication
  </h2>
 

 
    <table style="width: 1000px;" ><center> 
   

            <tr>
                
                <td>Project id</td>
                <td>Tiltle </td>
                <td>Team</td>
                <td>Dataset</td>
                <td>Section</td>
                <td>Guide</td>
                <td>Approved</td>
                <td>Status</td>
                <td>Supporting Documents</td>
                <td>Contact</td>
                <td>More Details</td>
            </tr>
            <% tracking.forEach(function (students) { %>
            <tr>  
                <td><%=  students.projectid%>     </td> 
                <td> <%= students.title%>      </td>
                <td> <%= students.team%>      </td>
                <td> <%= students.Dataset%>      </td>
                <td> <%= students.section%>      </td>
                <td> <%= students.guide%>      </td>
                <td> <%= students.Approved%>      </td>
                <td> <%= students.status%>      </td>
                 
               <td> <a href="<%= students.documents%>" >Click here</a></td>
                <td> <%= students.contact%>      </td>
                <td>  <button style="background-color: palevioletred;" formaction="/trackingdashboard">Click Here</button></td>
     
        <% }) %>
       </tr>   </center>
  </table>

     
        
   

 

 
</div>
    
